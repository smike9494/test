<div class="message-container">
        <% if(success && success.length > 0) { %>
        <div class="success-message">
          <div class="message-content">
              <%= success %> 
          <!-- <span class="close">&times;</span> -->
          </div>
        </div>
        <% } %> 
        <% if(error && error.length > 0) { %>
        <div class="error-message">
          <div class="message-content">
              <%= error %> 
              <!-- <span class="close">&times;</span>  -->
          </div>
        </div>
        <% } %> 
     </div> 

 <div style="background-color:#fff;" class="dash-logo-user">
        <div class="header-container">
            <div id="error-dash-nav-wrapper">
                <div id="errorcallToAction">
                    <a style="color:#000;  display:none;" id="toBack" href="/"><i class="far fa-arrow-alt-circle-left"></i> Back to <span class="ctabtn">VIPODAZA</span></a>
                </div>
                <div class="logo-title">
                    <div class="title-container">
                        <a style="color:#000;" href="/"><h2 style="color:#000; border:none;" >VIPODAZA</h2></a>
                    </div>
                </div>
                <div  id="errorLogout">
                    <% if(currentUser) { %>
                    <a style="color:#000;" href="/logout">Logout <i class="far fa-arrow-alt-circle-right"></i></a>
                    <% } %>
                </div>
            </div>
          </div>
