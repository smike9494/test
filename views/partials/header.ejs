    <div class="message-container">
      <% if(success && success.length > 0) { %>
      <div class="success-message">
        <div class="message-content">
            <%= success %> 
        <!-- <span class="close">&times;</span> -->
        </div>
      </div>
      <% } %> 
      <% if(error && error.length > 0) { %>
      <div class="error-message">
        <div class="message-content">
            <%= error %> 
            <!-- <span class="close">&times;</span>  -->
        </div>
      </div>
      <% } %> 
    </div> 

    <div class="logo" >
      <div class="header-container">
        <div class="logo-container">
        <div class="time">
          <div class="time-time">
            <%= moment().format('LL');  %>
          </div>
        </div>
          <div class="logo-title">
            <div class="title-container">
              <a href="/"><h2>VIPODAZA</h2></a>
            </div>
          </div>
          <div class="search">
            <div class="search-box">
              <form action='/search' method='GET'>
                <i class="fa fa-search" aria-hidden="true"></i>
                <input type='text' name='search' placeholder='Search Headlines and Articles...'>
              </form>
            </div>
            <% if(currentUser) { %>
              <span class="user-rg"><a href=/profile/user/<%= currentUser.id %>>My Account <i class="fas fa-user-circle"></i></a></span>
          <%  } else { %>
            <!-- <span class="user-rg"><a href="/login">Sign In</a> / <a id="register" href="/register">Register</a></span> -->
            <span class="user-rg"><a href="/login">Sign In <i class="far fa-arrow-alt-circle-right"></i></a></span>

          <% } %>
          </div>
        </div>
        </div>
    </div>

